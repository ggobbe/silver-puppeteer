{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules/": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "editor.formatOnSave": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.printWidth": 120
}
